{
  "workbench.startupEditor": "none",
  "editor.suggestSelection": "first",
  "security.workspace.trust.untrustedFiles": "open",
  "files.autoSave": "onFocusChange",
  "explorer.compactFolders": false,
  "terminal.integrated.windowsEnableConpty": false,
  "editor.minimap.enabled": false,
  "editor.fontFamily": "'Cousine1', 'Fira Code Light', 'Jetbrains Mono', 'Roboto Mono', monospace",
  "terminal.integrated.fontFamily": "'Cousine1', 'Fira Code Light', 'Jetbrains Mono', 'Roboto Mono', monospace",
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
  "editor.tabSize": 2,
  "terminal.integrated.fontSize": 14,
  "editor.fontSize": 14,
  "explorer.confirmDelete": false,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.tabWidth": 2,
  "editor.insertSpaces": false,
  "editor.detectIndentation": false,
  "workbench.tips.enabled": false,
  "update.showReleaseNotes": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#fb0000",
    "terminalCursor.foreground": "#fb0000"
  },
  "terminal.integrated.cursorWidth": 2,
  "editor.cursorWidth": 1,
  "editor.codeLensFontFamily": "'Cousine1', 'Fira Code Light', 'Jetbrains Mono', 'Roboto Mono', monospace",
  "markdown.preview.fontFamily": "'Cousine1', 'Fira Code Light', 'Jetbrains Mono', 'Roboto Mono', monospace",
  "workbench.colorTheme": "Monokai Dimmed"
}
